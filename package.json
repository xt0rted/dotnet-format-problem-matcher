    "github",
    "problem-matcher",
    "annotations"
  ],
  "author": "xt0rted",
  "license": "MIT",
  "dependencies": {
    "@actions/core": "^1.11.1"
  },
  "devDependencies": {
    "@types/jest": "^28.1.7",
    "@types/node": "^22.10.6",
    "@vercel/ncc": "^0.36.1",
    "github-actions-problem-matcher-typings": "^1.1.0",
    "jest": "^28.1.3",
    "jest-circus": "^29.7.0",
    "ts-jest": "^28.0.8",
    "typescript": "^5.0.4"
  }
}
