{
  "name": "dotnet-format-problem-matcher",
  "version": "1.0.0",
  "private": true,
  "description": "Sets up a problem matcher for dotnet-format to create annotations for violations.",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "lint": "tsc --noEmit",
